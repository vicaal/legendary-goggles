{
  "Version": "2012-10-17",
  "Statement":
  [
      {
      "Effect": "Allow",
      "Action": [
        "dynamodb:GetItem"
      ],
      "Resource": [
        "arn:aws:dynamodb:*:*:table/${table}"
      ],
      "Condition": {
          "ForAllValues:StringLike": {
          "dynamodb:LeadingKeys": [ "${tenant}*" ]
        }
      }
    }
  ]
}